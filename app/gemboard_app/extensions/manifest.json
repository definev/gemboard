{
  "manifest_version": 3,
  "name": "Gemboard quick actions",
  "version": "1.0",
  "description": "Quick actions for opening Gemboard in a side panel",
  "minimum_chrome_version": "116",
  "side_panel": {
    "default_path": "gemboard.html"
  },
  "content_scripts": [],
  "permissions": [
    "sidePanel",
    "contextMenus"
  ],
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}